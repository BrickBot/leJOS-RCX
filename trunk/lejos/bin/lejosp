#!/bin/sh

if [ "${JAVAP}" = "" ];
then
  JAVAP=javap
fi

if [ "${OSTYPE}" = "cygwin32" ];
then
  TINYVM_HOME="`dirname $0`/.."
  TINYVM_HOME="`cygpath -w ${TINYVM_HOME}`"
  JCCP="${TINYVMPATH};${TINYVM_HOME}\\lib\\classes.jar;${CLASSPATH}"
else
  TINYVM_HOME="`dirname $0`/.."
  JCCP="${TINYVMPATH}:${TINYVM_HOME}/lib/classes.jar:${CLASSPATH}"
fi

${JAVAP} -classpath ${JCCP} $@


