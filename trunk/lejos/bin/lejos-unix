#!/bin/sh

if [ "${OSTYPE}" = "cygwin32" ];
then
  TINYVM_HOME="`dirname $0`/.."
  TINYVM_HOME="`cygpath -w ${TINYVM_HOME}`"
else
  TINYVM_HOME="`dirname $0`/.."
fi

TINYVMOPTIONS="-Dtinyvm.write.order=LE ${TINYVMOPTIONS}"
TINYVMOPTIONS="-Dtinyvm.loader=lejosrun-unix ${TINYVMOPTIONS}"

export TINYVMOPTIONS
export TINYVM_HOME

${TINYVM_HOME}/bin/aux-linker $@

